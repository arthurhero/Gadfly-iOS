  pod 'GooglePlaces', '~> 2.5'

  # Pods for Gadfly

  target 'GadflyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GadflyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
